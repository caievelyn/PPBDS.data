citHeader("To cite 'PPBDS.data' in publications use:")

citEntry(
  entry    = "Manual",
  title    = "PPBDS.data",
  author   = person(given = "David",
                    family = "Kane",
                    role = c("aut", "cre"),
                    email = "dkane@fas.harvard.edu"),
  year = "2020",
  url      = "https://github.com/davidkane9/PPBDS.data",
  textVersion = paste("David Kane. 2020. 'PPBDS.data'. R package version 0.1.0, <https://github.com/davidkane9/PPBDS.data>."

  )
)
